---
title: {{meta.title}}
layout: root.hbs

---

<h1>{{meta.title}}</h1>
<h2>{{meta.datetime}}</h2>

{{#each pageData.spreads}}
	<section data-spread="{{{this.id}}}">
	{{#each this.pages}}
		<article data-page="{{{this.id}}}" data-type="{{{this.type}}}">
			<a href="page/{{{this.id}}}.html">
				<img src="img/pages/thumbs/{{{this.thumb}}}" alt="{{{this.title}}}" width="100">
				<h1>{{{this.id}}}</h1>
				<h2>{{{this.title}}}</h2>
				<p>{{{this.synopsis}}}</p>
			</a>
		</article>
	{{/each}}
	</section>
{{/each}}